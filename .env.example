# Discord Bot Configuration
# Copy this file to .env and fill in your actual values

# Your Discord bot token (required)
BOT_TOKEN=your_discord_bot_token_here

# Database file path (optional, defaults to notes_bot.db)
DATABASE_FILE=discord_notes_bot.db

# Logging configuration (optional)
LOG_LEVEL=INFO
LOG_FILE=discord_bot.log

# Timezone for reminders (optional, defaults to UTC)
REMINDER_TIMEZONE=UTC

# Example values:
# BOT_TOKEN=MTIzNDU2Nzg5MDEyMzQ1Njc4OQ.GhIjKl.MnOpQrStUvWxYzAbCdEfGhIjKlMnOpQrStUvWx
# DATABASE_FILE=my_notes.db
# LOG_LEVEL=DEBUG
# LOG_FILE=bot_debug.log
# REMINDER_TIMEZONE=America/New_York